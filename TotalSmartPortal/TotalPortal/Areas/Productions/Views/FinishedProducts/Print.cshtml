@using MvcReportViewer;
@model TotalPortal.ViewModels.Helpers.PrintViewModel

@Html.MvcReportViewerFluent("/TotalSmartPortal/" + (Model.Id > 0 ? "FinishedProductSheet" : "FinishedProductByWorkshift")).ReportParameters(new { Id = @Model.Id, DetailID = @Model.DetailID, CurrentHost = HttpContext.Current.Request.Url.Host }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false })