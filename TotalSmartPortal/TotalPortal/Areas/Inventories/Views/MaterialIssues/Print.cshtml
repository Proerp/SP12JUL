@using TotalBase.Enums;
@using MvcReportViewer;
@model TotalPortal.ViewModels.Helpers.PrintViewModel

@Html.MvcReportViewerFluent("/TotalSmartPortal/" + (@Model.NmvnTaskID == GlobalEnums.NmvnTaskID.MaterialStaging ? "MaterialGIRSheet" : "MaterialIssueSheet")).ReportParameters(new { Id = (@Model.NmvnTaskID == GlobalEnums.NmvnTaskID.MaterialStaging ? (int)GlobalEnums.NmvnTaskID.MaterialIssue : @Model.Id), DetailID = @Model.Id, CurrentHost = HttpContext.Current.Request.Url.Host }).Attributes(new { Height = 1100, Width = 1150, style = "border: none" }).ControlSettings(new ControlSettings() { ShowParameterPrompts = false })