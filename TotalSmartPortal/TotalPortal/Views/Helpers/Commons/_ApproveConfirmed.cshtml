@model TotalDTO.BaseDTO

<div class="single-full-panel">
    @if (Model.Approvable || Model.UnApprovable)
    {
        <button type="submit" form="MainForm" class="k-button custom-button"> <img src="~/Content/Images/Mains/@(@Model.Approvable ? "Check-32-CHY.png" : "cloud-32-NoCheck.png")" alt="" /> Xác nhận @(Model.Approvable ? Model.ApproveWarning : Model.UnApproveWarning) dữ liệu </button>
    }
    else
    {
        <div id="div-DeleteConfirm">
            <img src="~/Content/Images/New/alert1.png" alt="Alert" /><label>Dữ liệu này đã bị khóa.</label>
        </div>
    }
</div>